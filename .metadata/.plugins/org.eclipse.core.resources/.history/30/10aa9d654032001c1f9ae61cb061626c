package polynomial;

public class Monomial {
  private final int degree;
  private final int coEfficient;
  
  
  Monomial(int ... polyTerms){
    this.degree=degree;
    this.coEfficient=coEfficient;
  }

  public int getCoEfficient() {
    return this.coEfficient;
  }
  
  public int getPower() {
    return this.degree;
  }
}
